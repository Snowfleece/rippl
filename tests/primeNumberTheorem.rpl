isPrime :: int -> bool
isPrime n =
	let max = quot n 2 in
	let range = [2...max] in
	let divisors = [x | x over range, mod n x == 0] in
	len divisors == 0	



primeNumberTheorem :: int -> int
primeNumberTheorem n =
	let range = [(n+1)...2*n] in
	let oddRange = [x | x over range, mod x 2 != 0] in
	foldl (fun prev -> fun curr -> if isPrime prev then prev else curr) 
			(head oddRange) oddRange 
