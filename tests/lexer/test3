is_prime n =
	let max = n / 2 in
	let range = [2...max] in
	let divisors = [x | x over range, n % x == 0] in 
	len divisors == 0

prime_number_theorem n =
	let range = [(n+1)...2*n] in
	let odd_range = [x | x over range, x % 2 != 0 ] in
	foldl (fun prev -> fun curr -> if is_prime then prev curr)
		(head odd_range) odd_range 
